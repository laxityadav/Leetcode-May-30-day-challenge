class Solution {
    public int numJewelsInStones(String J, String S) {
        int ans=0;
        for(int i=0;i<S.length();i++)
        {
            char ch=S.charAt(i);
            for(int j=0;j<J.length();j++)
            {
                if(ch==J.charAt(j)){
                    ans++;
                    break;
                }
            }
        }
        return ans;
    }
}
